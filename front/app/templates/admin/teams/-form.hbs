<form {{action "save" on="submit"}} class="create-event">
  <div>
    <label>
      Nombre
      {{input type="text" value=model.name}}
    </label>
  </div>
  <div class="divider"></div>
  <div>
    <label>
      Configuraciones
      {{model-selector modelName='config' value=model.configs labelPath="name" multiple=true}}    
     </label>
  </div>
  <div class="divider"></div>
  <div>
    <input type="submit" value="Guardar">
  </div>
</form>
